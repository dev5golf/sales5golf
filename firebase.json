{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/admin.html"
      },
      {
        "source": "/list",
        "destination": "/list.html"
      },
      {
        "source": "/detail",
        "destination": "/detail.html"
      }
    ]
  }
}