{
    "name": "sales5golf",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev -p 3000",
        "build": "next build",
        "build:dev": "next build",
        "build:prod": "next build",
        "start": "serve out -l 3000",
        "deploy:dev": "npm run build:dev && firebase hosting:channel:deploy dev --config firebase.dev.json",
        "deploy:staging": "npm run build:dev && firebase hosting:channel:deploy staging --config firebase.staging.json",
        "deploy:prod": "npm run build:prod && firebase deploy --only hosting",
        "deploy:prod:channel": "npm run build:prod && firebase hosting:channel:deploy prod --config firebase.json",
        "prebuild": "npm run clean",
        "clean": "rimraf out .next",
        "build:analyze": "ANALYZE=true npm run build",
        "build:watch": "next build --watch",
        "create-user": "node scripts/create-user.js",
        "create-course": "node scripts/create-course.js",
        "create-locations": "node scripts/create-locations.js"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.1",
        "@opentelemetry/api": "^1.9.0",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@types/papaparse": "^5.3.16",
        "@types/react-datepicker": "^7.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^17.2.1",
        "firebase": "^12.2.1",
        "firebase-admin": "^13.5.0",
        "html-to-image": "^1.11.13",
        "i18next": "^25.5.1",
        "lucide-react": "^0.544.0",
        "next": "14.2.5",
        "papaparse": "^5.5.3",
        "react": "18.3.1",
        "react-datepicker": "^8.7.0",
        "react-dom": "18.3.1",
        "react-hook-form": "^7.62.0",
        "react-i18next": "^15.7.3",
        "tailwind-merge": "^3.3.1",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^4.1.8"
    },
    "devDependencies": {
        "@types/html2canvas": "^1.0.0",
        "@types/node": "24.3.0",
        "@types/react": "19.1.11",
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "rimraf": "^5.0.10",
        "serve": "14.2.1",
        "tailwindcss": "^3.4.0",
        "typescript": "5.9.2"
    }
}
